      <hedline>
        <hl1>Wayne Awaits Flood Relief</hl1>
      </hedline>
      <byline class="print_byline">By LAWRENCE VAN GELDER</byline>
      <byline class="normalized_byline">VAN GELDER, LAWRENCE</byline>
      <dateline>TRENTON</dateline>
